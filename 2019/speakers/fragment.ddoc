DDOC =
$(DEFINE $(TITLE)_BLURB,
  $(T a, href="$(SPEAKER_URL)", $(T h1,, $(SPEAKER)))
  $(T table, class="tablediv",
    $(T tr,,
      $(T td,, <img class="stroke" src="$(SPEAKER_PIC)"  height="170" alt="$(SPEAKER)" />)
      $(T td,, $(BIO))
    )
  )
)


SPEAKER_PIC = $(BASE)/../2018/images/stock.png

BASE = ..
